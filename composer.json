{
	"name": "primehalo/primeusertopics",
	"type": "phpbb-extension",
	"description": "Adds a link to search user's topics next to the “Search user's posts” link on the user profile page. Also adds links to search your own topics on the UCP overview page and in the quick links of the page header.",
	"homepage": "https://github.com/primehalo/primeusertopics",
	"version": "1.1.3",
	"time": "2018-05-16",
	"license": "GPL-2.0-only",
	"authors": [
		{
			"name": "Ken F. Innes IV",
			"email": "primehalo@gmail.com",
			"homepage": "http://www.absoluteanime.com/",
			"role": "Lead Developer"
		}
	],
	"require": {
		"php": ">=5.3.3"
	},
	"extra": {
		"display-name": "Prime User Topics",
		"soft-require": {
			"phpbb/phpbb": ">=3.1.0,<3.3.0@dev"
		},
		"version-check": {
		    "host": "www.absoluteanime.com",
			"directory": "/_phpbb-extensions_/versioncheck",
			"filename": "primeusertopics.json",
			"ssl": true
		}
	}
}